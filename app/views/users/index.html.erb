<div class="container">
  <div class="row">
    <h3>Users</h3>
    <div class="column">
      <table class="table table-striped">
        <thead>
          <tr>
            <th class="text-center">Name</th>
            <th class="text-center">Email</th>
          </tr>
        </thead>
        <tbody>
          <% users.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
